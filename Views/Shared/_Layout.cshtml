<!DOCTYPE html>
<html translate="no">
<head>
    <meta name="google" content="notranslate">
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>@ViewBag.Title | Pizza System</title>
    <link rel="shortcut icon" type="image/png" href="@Url.Content("~/Images/favicon.ico")">
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    @Styles.Render("~/Content/css")
    @RenderSection("Styles", false)
</head>
<body class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper">

        <header class="main-header">
            <nav class="navbar navbar-default fixed-top" style="display: block;">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="push-menu" role="button">
                    <span class="menu-text" style="font-family: Arial; font-weight:bold; margin-left:5px"></span>
                    <span class="sr-only">Toggle navigation</span>
                </a>

                <div class="div-logo">
                    <!-- Logo -->
                    <a href="#" class="logo navbar-default">
                        <span class="logo-mini"><img class="inlineBlock img-responsive" src="@Url.Content("~/Images/pizza_web.png")" /></span>   <!-- mini logo for sidebar mini 50x50 pixels -->
                        <span class="logo-lg"><img class="inlineBlock img-responsive" src="@Url.Content("~/Images/pizza_web.png")" /></span>     <!-- logo for regular state and mobile devices -->
                    </a>
                </div>

                <div class="navbar-custom-menu" style="margin: 15px;">
                    <a href="@Url.Action("LogOut","Login")" style="color: white;"><span>Cerrar sesión </span><i class="fas fa-sign-out-alt"></i></a>
                </div>
            </nav>


        </header>
        <!-- Left side column. contains the logo and sidebar -->
        <aside class="main-sidebar">
            @{ Html.RenderAction("Menu", "Menu"); }
        </aside>

        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <section class="content" id="content" style="padding-top:0px">
                @RenderBody()
            </section>
            <!-- /.content -->
        </div>
        <!-- /.content-wrapper -->
        <footer class="main-footer fixed-bottom" style="text-align:center">
            <p style="height: 10px;">
                Copyright &copy; @DateTime.Now.Year - William Abrego.
            </p>
        </footer>
        <div id="loading" tabindex="-1" style="display: block;">
            <div>
                <img style=" position: fixed; left: 0px; top: 0px; width: 100%; height: 100%; z-index: 9999; background: url(/Images/Comp-8.gif) 50% 50% no-repeat rgb(249 249 249 / 73%);" />
            </div>
        </div>
    </div>
    <!-- ./wrapper -->
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/popper")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/adminlte")
    @Scripts.Render("~/bundles/sweetAlert")
    @RenderSection("scripts", required: false)

</body>
</html>

