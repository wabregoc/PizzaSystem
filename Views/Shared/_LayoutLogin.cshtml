<!DOCTYPE html>
<html>
<head>
    <meta name="google" content="notranslate">
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Login | Pizza System</title>
    <link rel="stylesheet" type="text/css" href="styles.css" />
    <link href="~/Content/login-styles.css" rel="stylesheet" />
</head>
<body>
    <div style="padding-top:50px">
        @RenderBody()
    </div>

    <div id="loading" tabindex="-1" style="display: block;">
        <div>
            <img style=" position: fixed; left: 0px; top: 0px; width: 100%; height: 100%; z-index: 9999; background: url(/Images/Comp-8.gif) 50% 50% no-repeat rgb(249 249 249 / 73%);" />
        </div>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/popper")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/adminlte")
    @Scripts.Render("~/bundles/sweetAlert")
    @RenderSection("scripts", required: false)

</body>
</html>
